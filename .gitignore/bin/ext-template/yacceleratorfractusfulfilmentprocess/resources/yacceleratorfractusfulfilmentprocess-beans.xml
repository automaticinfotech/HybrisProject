<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<!--
	  - Events: Order 
	  -->

	<bean class="de.hybris.platform.yacceleratorfractusfulfilmentprocess.order.events.SubmitFractusOrderEvent" type="event"
		template="resources/pojo/SubmitOrderEvent-template.vm">
		<property name="order" type="de.hybris.platform.core.model.order.OrderModel"/>
	</bean>

	<!-- Fractus extends commerceservices' CommerceCheckoutParameter here. -->
    <bean class="de.hybris.platform.commerceservices.service.data.CommerceCheckoutParameter">
        <property name="order" type="de.hybris.platform.core.model.order.OrderModel">
            <description>The CartModel</description>
        </property>
    </bean>

</beans>
